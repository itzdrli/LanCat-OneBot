{
    "$schema": "./manifest_schema.json",
    "manifest_version": 4,
    "type": "extension",
    "name": "蓝猫OneBot",
    "slug": "lancat_onebot",
    "description": "为其它应用提供OneBot接口。",
    "version": "0.0.01",
    "icon": "./icon.png",
    "authors": [
        {
            "name": "MeowSauce,
            "link": "https://github.com/MeowSauce"
        }
    ],
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    },
    "repository": {
        "repo": "MeowSauce/LanCat-OneBot",
        "branch": "main",
        "release": {
            "tag": "0.0.1",
            "file": "index.zip"
        }
    }
}